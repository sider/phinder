<?xml version="1.0"?>
<ruleset name="Phinder Coding Standards">
  <arg name="ignore" value="src/lib/Parser/PatternParser/*,test/res/php/*" />
  <file>src/lib</file>
  <file>test</file>
  <rule ref="PEAR">
    <exclude name="PEAR.Commenting.ClassComment.Missing" />
    <exclude name="PEAR.Commenting.FileComment.Missing" />
    <exclude name="PEAR.Commenting.FunctionComment.Missing" />
  </rule>
</ruleset>
